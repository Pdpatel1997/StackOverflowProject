@model List<StackOverflowProject.ViewModels.QuestionViewModel>
@{
    ViewBag.Title = "Questions";
}


<div class="row">
    <div class="col-sm-12">
        <table class="table">
            @foreach (var item in Model)
            {
                Html.RenderPartial("Question", item);
            }
        </table>

        @if (Session["CurrentUserName"] != null)
        {
            <a href="/question/create" class="btn btn-primary">Add Question</a>
        }

    </div>
</div>

<script>
    $("a[href='/AllQuestions']").attr("id", "active");
</script>
